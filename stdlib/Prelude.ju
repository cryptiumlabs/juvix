mod Prelude where

-- it seems custom arrow syntax is not operational

let (->) = %Builtin.ArrowOmega

declare infixr (->) 1

declare infixr (:) 2

sig id : (a : ty) -> a -> a
let id x = x

sig const : (a : ty) -> (b : ty) -> a -> b -> a
let const x = \_ -> x

sig flip : (a : ty) -> (b : ty) -> (c : ty) -> b -> a -> c
let flip f x y = f y x

declare infixl (.) 9

sig (.) : (a : ty) -> (b : ty) -> (c : ty) -> (b -> c) -> (a -> b) -> (a -> c)
let (.) f g = \x -> f (g x)
