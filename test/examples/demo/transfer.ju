open Prelude
open Prelude.Michelson

sig cons-pair : list operation -> nat -> pair (list operation) nat
let cons-pair = %Michelson.pair

sig nil : list operation
let nil = %Michelson.nil

sig main : pair unit-t unit-t -> pair (list operation) unit-t
let main = \ps ->
  let contract = address-to-contract unit-t sender in
  let op = transfer-tokens unit balance contract in
  cons-pair (cons operation nil) (cdr ps)
