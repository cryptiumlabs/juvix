mod If where

open Prelude

open Michelson

sig main : pairTy int int -> pairTy (list operation) int
let main = \pair ->
  cons-pair
    (list operation)
    int
    nil
    (if2 int
         (compare 0 (car int int pair))
         (cdr int int pair)
         (add int (car int int pair) (cdr int int pair)))